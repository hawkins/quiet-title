{
    "manifest_version": 2,
    "name": "Quiet Title",
    "version": "0.1",
    "author": "Josh Hawkins",
    "description": "Lowercases YouTube video titles",
    "permissions": [
        "https://www.youtube.com/*",
        "http://www.youtube.com/*"
    ],
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*", "http://www.youtube.com/*"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ]
}
